{
  "name": "RareEdition",
  "description": "read data from specific websites",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_icon": "re-black-logo.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["content.js", "fill.js"],
      "matches": ["https://www.psacard.com/"]
    }
  ]
}
